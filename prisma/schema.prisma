generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Config {
  id Int @id @default(autoincrement())
}

model WakaEntry {
  date   String @id @unique
  uv     Float
  uvText String
}
