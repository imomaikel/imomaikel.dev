generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

enum Status {
  ONLINE
  OFFLINE
}

model Config {
  id     Int    @id @default(autoincrement())
  status Status @default(OFFLINE)
}

model WakaEntry {
  date   String @id @unique
  uv     Float
  uvText String
}
